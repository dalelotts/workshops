<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="decoyDuck" class="DuckUSim.DecoyDuck">
        <constructor-arg index="0" ref="noFlight"/>
        <constructor-arg index="1" ref="noQuack"/>
    </bean>
    <bean id="defaultQuack" class="DuckUSim.DefaultQuackBehavior"/>
    <bean id="mallardDuck" class="DuckUSim.MallardDuck">
        <constructor-arg index="0" ref="wingFlight"/>
        <constructor-arg index="1" ref="defaultQuack"/>
    </bean>
    <bean id="modelDuck" class="DuckUSim.ModelDuck">
        <constructor-arg index="0" ref="rocketFlight"/>
        <constructor-arg index="1" ref="defaultQuack"/>
    </bean>
    <bean id="noFlight" class="DuckUSim.NoOpFlightBehavior"/>
    <bean id="noQuack" class="DuckUSim.NoOpQuackBehavior"/>
    <bean id="redheadDuck" class="DuckUSim.RedheadDuck">
        <constructor-arg index="0" ref="wingFlight"/>
        <constructor-arg index="1" ref="defaultQuack"/>
    </bean>
    <bean id="rocketFlight" class="DuckUSim.RocketFlightBehavior"/>
    <bean id="rubberDuck" class="DuckUSim.RubberDuck">
        <constructor-arg index="0" ref="noFlight"/>
        <constructor-arg index="1" ref="squeakQuack"/>
    </bean>
    <bean id="squeakQuack" class="DuckUSim.SqueakQuackBehavior"/>
    <bean id="wingFlight" class="DuckUSim.WingFlightBehavior"/>
</beans>